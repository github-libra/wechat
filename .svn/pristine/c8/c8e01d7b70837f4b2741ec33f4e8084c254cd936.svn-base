<div class = "wrap"  >
	<div class = "all-middle">
			<span id = "mainscreen-digit">{{humidityValue}}</span>
			<span id = "centigrade-sign">%</span>
			<div class = "mode-sign"></div>
			<div class = "trending-sign"></div>
			<a href = #/temperature><div class = "switch-to-temperature-btn"></div></a>
	</div>
</div>

<div class = "up-snow-btn" ng-click = "addHumidity(humidityValue)"></div>
<div class = "down-snow-btn" ng-click = "minusHumidity(humidityValue)"></div>
